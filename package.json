{
  "name": "gestionProgramas-frontend",
  "version": "1.0.0",
  "base_version": "4.3.0",
  "node_version": "20.11.1",
  "type": "templateFrontend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage",
    "prod": "ng serve --host 0.0.0.0 --port 4200 --configuration=production",
    "dev": "ng serve --host 0.0.0.0 --port 4200",
    "local": "ng serve --host 0.0.0.0 --port 4200 --configuration=local",
    "build": "ng build --configuration=production --build-optimizer=true --aot=true --named-chunks",
    "build-dev": "ng build --configuration=development --build-optimizer=true --aot=true --named-chunks --base-href /gestionProgramas/",
    "prettier-c": "prettier --check .",
    "prettier-w": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@ng-idle/core": "^13.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@sentry/angular": "^8.9.2",
    "jquery": "^3.7.1",
    "ngx-spinner": "^16.0.2",
    "ngx-translate-multi-http-loader": "^16.0.1",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "2.1.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "typescript": "~5.1.3"
  }
}
